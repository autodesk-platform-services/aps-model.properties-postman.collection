											" ",
											"         //url encoded",
											"        pm.environment.set(\"first_test_model_version\",  first_test_model_version);",
											"        pm.environment.set(\"second_test_model_version\",  second_test_model_version);",
											"        ",
											"    }else{",
											"        ",
